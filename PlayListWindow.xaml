<Window x:Class="MikuMikuVLC.PlayListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MikuMikuVLC"
        mc:Ignorable="d"
        Title="PlayListWindow" Height="386.975" Width="266.807" MouseDown="Window_MouseDown">
    <Window.Background>
        <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/MusicLsWindow.png"/>
    </Window.Background>
    <Grid Margin="0,0,267,0">
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="278" Margin="32,31,-231,0" VerticalAlignment="Top" Width="199" Background="{x:Null}" DragEnter="listBox_DragEnter" Drop="listBox_Drop" MouseDoubleClick="listBox_MouseDoubleClick" KeyDown="listBox_KeyDown"/>
        <Image x:Name="SaveList" HorizontalAlignment="Left" Height="20" Margin="132,314,-229,0" VerticalAlignment="Top" Width="97" MouseEnter="BtnMouseEnter" MouseLeave="BtnMouseLeave" MouseDown="SaveList_MouseDown"/>
        <Image x:Name="Close" HorizontalAlignment="Left" Height="32" Margin="220,9,-252,0" VerticalAlignment="Top" Width="32" MouseEnter="BtnMouseEnter" MouseLeave="BtnMouseLeave" MouseDown="Close_MouseDown"/>

    </Grid>
</Window>
